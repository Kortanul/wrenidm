{
  "$schema": "http:\/\/json-schema.org\/draft-04\/schema#",
  "description": "New user details",
  "type": "object",
  "properties": {
    "redirect_uri": {
      "description": "OAuth\/OIDC redirect URI used to process the authorization code",
      "type": "string"
    },
    "nonce": {
      "description": "One-time use random value",
      "type": "string"
    },
    "provider": {
      "description": "OAuth\/OIDC IdP name",
      "type": "string"
    },
    "code": {
      "description": "OAuth\/OIDC authorization code",
      "type": "string"
    },
    "id_token": {
      "description": "OAuth\/OIDC ID Token",
      "type": "string"
    },
    "user": {
      "default": {

      },
      "description": "User Object",
      "type": "object"
    },
    "access_token": {
      "description": "OAuth\/OIDC Access Token",
      "type": "string"
    }
  },
  "definitions": {
    "providers": {
      "type": "array",
      "items": {
        "type": "object",
        "oneOf": [

        ]
      }
    }
  },
  "socialRegistrationEnabled": false,
  "registrationForm": null,
  "registrationProperties": {
    "properties": {
      "mail": {
        "title": "Email Address",
        "viewable": true,
        "type": "string",
        "searchable": true,
        "userEditable": true,
        "policies": [
          {
            "policyId": "valid-email-address-format"
          }
        ]
      },
      "sn": {
        "title": "Last Name",
        "viewable": true,
        "type": "string",
        "searchable": true,
        "userEditable": true
      },
      "givenName": {
        "title": "First Name",
        "viewable": true,
        "type": "string",
        "searchable": true,
        "userEditable": true
      },
      "userName": {
        "title": "Username",
        "viewable": true,
        "type": "string",
        "searchable": true,
        "userEditable": true,
        "minLength": 1,
        "policies": [
          {
            "policyId": "unique"
          },
          {
            "policyId": "no-internal-user-conflict"
          },
          {
            "policyId": "cannot-contain-characters",
            "params": {
              "forbiddenChars": [
                "\/"
              ]
            }
          }
        ]
      }
    },
    "required": [
      "mail",
      "sn",
      "givenName",
      "userName"
    ]
  },
  "registrationPreferences": {
    "marketing": {
      "description": "Send me special offers and services",
      "type": "boolean"
    }
  }
}